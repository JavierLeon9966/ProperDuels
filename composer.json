{
  "name": "javierleon9966/properduels",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "description": "A properly made duels plugin for PocketMine-MP",
  "type": "project",
  "license": "GPL-3.0-only",
  "require": {
    "pocketmine/pocketmine-mp": "^5.0.0",
    "sof3/libasynql": "^4.2.0",
    "muqsit/simple-packet-handler": "dev-pm5",
    "paroxity/commando": "master"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "phpstan/extension-installer": "^1.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "paroxity/commando",
        "version": "master",
        "autoload": {
          "classmap": ["src/"]
        },
        "source": {
          "url": "https://github.com/paroxity/commando",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "autoload": {
    "psr-0": {
      "JavierLeon9966\\ProperDuels\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}